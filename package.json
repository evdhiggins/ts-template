{
    "name": "ts-template",
    "version": "0.0.1",
    "description": "A simple TS project template with linting and testing support",
    "main": "built/index.js",
    "type": "module",
    "scripts": {
        "build": "tsc -p ./tsconfig.build.json",
        "lint": "eslint '*/**/*.{ts}' --quiet --fix",
        "test": "jest"
    },
    "keywords": [
        "typescript",
        "ts",
        "util",
        "utilities",
        "helpers"
    ],
    "author": "Evan Higgins <evdhiggins@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "29.2.0",
        "@typescript-eslint/eslint-plugin": "5.41.0",
        "@typescript-eslint/parser": "5.41.0",
        "eslint": "8.26.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-prettier": "4.2.1",
        "jest": "29.2.2",
        "prettier": "2.7.1",
        "ts-jest": "29.0.3",
        "ts-node": "10.9.1",
        "typescript": "4.8.4"
    },
    "volta": {
        "node": "18.12.0"
    }
}
